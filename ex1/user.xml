<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
chan press;</declaration>
	<template>
		<name x="5" y="5">Light</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="178" y="8">
		</location>
		<location id="id1" x="-8" y="8">
		</location>
		<location id="id2" x="-221" y="8">
		</location>
		<init ref="id2"/>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-25" y="119">press?</label>
			<nail x="0" y="178"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="10" y="-9">press?</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-203" y="-9">press?</label>
		</transition>
	</template>
	<template>
		<name x="5" y="5">User</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id3" x="0" y="0">
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id3"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="110" y="0">press!</label>
			<nail x="102" y="-59"/>
			<nail x="102" y="68"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
user = User();
machine = Light();
// List one or more processes to be composed into a system.
system user, machine;
    </system>
	<queries>
	</queries>
</nta>
